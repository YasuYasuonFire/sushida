{
    "family": "sushida-prod-task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "sushida-prod-container",
            "image": "<IMAGE1_NAME>",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::${AWS_ACCOUNT_ID}:role/ecsTaskExecutionRole"
}
